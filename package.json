{
  "name": "@geospoc/v-grid-layout",
  "version": "0.1.0",
  "description": "A draggable and resizable grid layout, as a Vue component",
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "main": "dist/v-grid-layout.cjs.min.js",
  "unpkg": "dist/v-grid-layout.umd.min.js",
  "scripts": {
    "test": "ava",
    "test:snapshot": "ava --update-snapshots",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "rollup -c build/rollup.config.js",
    "bundle:min": "rollup -c build/rollup.min.config.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lintfix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
    "lint:eslint": "eslint \"{,!(node_modules|dist|examples)/**/}*.{js,vue}\"",
    "lint:eslint:fix": "eslint --fix \"{,!(node_modules|dist|examples)/**/}*.{js,vue}\"",
    "lint:prettier": "prettier --check \"{,!(node_modules|dist|examples)/**/}*.{js,vue}\"",
    "lint:prettier:fix": "prettier --write \"{,!(node_modules|dist|examples)/**/}*.{js,vue}\"",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "element-resize-detector": "^1.2.1",
    "interactjs": "^1.9.22"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@geospoc/prettier-config-standard": "^1.0.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@vue/test-utils": "^1.1.0",
    "ava": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "prettier": "^2.1.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^3.0.0",
    "rollup": "^2.28.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.6",
    "shipjs": "^0.21.1",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "url": "git+https://github.com/geospoc/v-grid-layout",
    "type": "git"
  },
  "author": {
    "name": "GeoSpoc Developers",
    "email": "developers@geospoc.com",
    "url": "https://geospoc.io"
  },
  "contributors": [
    {
      "name": "Vinayak Kulkarni",
      "email": "inbox.vinayak@gmail.com",
      "url": "https://vinayakkulkarni.dev"
    }
  ],
  "keywords": [
    "grid",
    "vuejs",
    "drag",
    "draggable",
    "resize",
    "resizable",
    "fluid",
    "responsive"
  ],
  "bugs": {
    "url": "https://github.com/geospoc/v-grid-layout/issues"
  },
  "homepage": "https://github.com/geospoc/v-grid-layout#readme"
}
